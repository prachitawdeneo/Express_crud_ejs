    <form className="container" method="post" action="/postdata">
<div class="mb-3">
<label for="title" class="form-label">Title</label>
<input type="text" class="form-control" id="title" name="title" ref={titleInp}/>

</div>
<div class="mb-3">
<label for="des" class="form-label">Description</label>
<textarea type="text" class="form-control" id="des" name="des" ref={desInp}></textarea>
</div>
<div class="mb-3">
    
    <input type="hidden" class="form-control" id="csrf" name="csrf" value="<%= csrf_token %>"/>
    
